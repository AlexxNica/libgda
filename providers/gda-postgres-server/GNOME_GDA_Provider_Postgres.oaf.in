<oaf_info>

<oaf_server iid="OAFIID:GNOME_GDA_Provider_Postgres_ConnectionFactory"
            type="exe"
            location="gda-postgres-srv">
	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:GDA/ConnectionFactory:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="description" type="string"
	               _value="GDA Datasource Access for Postgres"/>
	<oaf_attribute name="gda_params" type="stringv">
		<item value="HOST"/>
		<item value="DATABASE"/>
		<item value="PORT"/>
		<item value="OPTIONS"/>
		<item value="TTY"/>
		<item value="LOGIN"/>
		<item value="PASSWORD"/>
	</oaf_attribute>
</oaf_server>

</oaf_info>
