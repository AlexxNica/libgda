<?xml version="1.0"?>
<data-set-spec>
  <parameters>
    <!-- Connection parameters -->
    <parameter id="HOST" _name="Database server" _descr="Host which has the database server running (for servers running on unix domain sockets, enter the socket's path (usually /tmp), or leave this fiend empty)" gdatype="string"/>
    <parameter id="PORT" _name="Port" _descr="Database server port (for servers running on unix domain sockets, enter the socket's file name extension (usually 5432), or leave this fiend empty)" gdatype="integer"/>
    <parameter id="OPTIONS" _name="Options" _descr="Extra connection options" gdatype="string"/>
    <parameter id="ADM_LOGIN" _name="Administrator login" _descr="Login name (which has the rights to create databases)" gdatype="string"/>
    <parameter id="ADM_PASSWORD" _name="Administrator password" gdatype="string"/>
    <parameter id="REQUIRESSL" _name="Require SSL" _descr="Whether to require SSL or not when connecting" gdatype="boolean"/>

    <!-- DSN parameters -->
    <parameter id="DATABASE" _name="Database name" _descr="The name of a database to create" gdatype="string" null-ok="FALSE"/>
    <parameter id="ENCODING" _name="Encoding" _descr="Character set encoding to use in the new database" gdatype="string" source="csets:0"/>
    <parameter id="OWNER" _name="Owner" _descr="The name of the database user who will own the new database" gdatype="string"/>
    <parameter id="TEMPLATE" _name="Template" _descr="The name of the template from which to create the new database" gdatype="string"/>
    <parameter id="TABLESPACE" _name="Tablespace" _descr="The name of the tablespace that will be associated with the new database" gdatype="string"/>
  </parameters>
