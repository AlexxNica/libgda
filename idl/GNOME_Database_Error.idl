#ifndef __GDA_Error_idl__
#define __GDA_Error_idl__ 1

module GDA {
	struct Error {
		string description;
		long   number;
		string source;
		string sqlstate;
		string nativeMsg;
	};
	
	typedef sequence<Error> ErrorSeq;
	
	exception DriverError {
		ErrorSeq errors;
		string   realcommand;
	};
	exception NotSupported {
		string errormsg;
	};
};

#endif
