<?xml version="1.0"?>
<!DOCTYPE query SYSTEM "xmlquery.dtd">

<query>
  <select>
    <targetlist>
      <target name="tab_a" id="t01"/>
      <joinlist>
        <join type="inner">
          <target name="tab_b" id="t02"/>
          <on>
            <eq>
              <field source="t01" name="fa"/>
              <field source="t02" name="fb"/>
            </eq>
          </on>
        </join>
      </joinlist>
    </targetlist>
    <valuelist>
      <value id="v01">
        <field source="t01" name="fa"/>
      </value>
      <value id="v02">
        <field source="t01" name="fb"/>
      </value>
      <value id="v03">
        <field source="t02" name="fc"/>
      </value>
    </valuelist>
  </select>
</query>


